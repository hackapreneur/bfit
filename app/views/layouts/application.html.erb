<!DOCTYPE html>
<html>
  <head>
    <title>Jeromeskee Coaching</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <center>
        <%= image_tag "https://s3.amazonaws.com/jeromeskeecoaching/headerbanner.jpg" %>
        </center>
    <div class="container">
            <% flash.each do |name, msg| %>
                <%= content_tag(:div, msg, class: "alert alert-#{name}") %>
            <% end %>
            <%= yield %>
    </div>

    <%= render 'layouts/footer' %>

  </body>
</html>