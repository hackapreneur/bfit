<%= form_for @video do |f| %>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div>
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
    <div>
      <%= f.label :vimeoUrl %>
      <%= f.text_field :vimeoUrl %>
    </div>
    <div>
      <%= f.submit %>
    </div>
<% end %>